	<script src="../js/mui.min.js"></script>
	<script>
	 	mui.init();
	 	var dpr=window.devicePixelRatio;
	 	console.log(dpr);
		//物流页面js事件
			mui('body .trans-content').on('tap','.mui-table-view-cell',function(){
				var down = this.querySelector('.iconfont.down-arrow');
				var up = this.querySelector('.up-arrow');
				if(down.style.display == 'block' && up.style.display == 'none'){
					down.style.display = 'none';
					up.style.display = 'block';	
				}else{
					down.style.display = 'block';
					up.style.display = 'none';	
				}
			})
			mui('#logistic-play')[0].addEventListener('tap',function(){
				//打开物流视频列表页面
				this.children[0].style.color="#ec4b1f";
			})
			//生产页面js事件
			mui('body .controll').each(function(index,item){
				var chatButtons = mui('.chat-button');//获取评论收藏面板
				var chatContent = mui('.chat-content');//获取评论内容
				this.addEventListener('tap',function(){
					this.classList.add('clicked');
					if(chatButtons[index].style.display=="none"&& chatContent[index].style.display=="none"){
						chatButtons[index].style.display="block";
					}else{
						chatButtons[index].style.display="none";
					}
					mui('.collectButton')[index].addEventListener('tap',function(){//点击收藏事件
						if(this.classList.contains('clicked')){
							this.classList.remove('clicked');
						}else{
							this.classList.add('clicked');
						}
					})
					mui('.chatButton')[index].addEventListener('tap',function(){//点击评论事件
						this.classList.add('clicked');
						chatContent[index].style.display="block";
						this.parentNode.parentNode.style.display="none";
					})
					
					if(chatContent[index].style.display=="block"){
						chatContent[index].style.display="none";
					}
				})
			})
			
			mui('.produce-center .button').each(function(index,item){
				this.addEventListener('tap',function(){
					mui('.buttoned')[index].style.display="block";
					var currCalendar = mui('.calendar')[index];
					currCalendar.style.display ="block";
					currCalendar.addEventListener('tap',function(){
						this.style.display="none";
						mui('.buttoned')[index].style.display="none";
					})
				})
			})
			mui('.produce-center .play-button').each(function(index,item){
				this.addEventListener('tap',function(){
					mui('.vedio-layer')[index].style.display="block";
				})
			})
			//质检页面的js事件
			mui('.banner')[0].addEventListener('tap',function(){
				mui('#vertify-body').popover('toggle');
			})
	</script>
