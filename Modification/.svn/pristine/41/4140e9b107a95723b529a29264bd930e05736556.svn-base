<!doctype html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
		<!--<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />-->
		<script src="../js/flexible.js"></script>
	    <link href="../css/mui.min.css" rel="stylesheet"/>
	    <link rel="stylesheet" href="../css/normalize.css" />
	    <link rel="stylesheet" href="../fonts/iconfont.css" />
		<style>
			ul{
				margin:0;
				padding:0;
			}
			ul li{
				
				list-style: none;
			}
			button{
				padding:0;
				margin:0;
			}
			body{
				font-family:"微软雅黑" !important;
				background-color:#e8e8e8 !important;
				color:#2b2b2b !important;
			}
			b{
				font-weight:normal;
			}
			.mui-content{
				margin-top:0.426666rem !important;
				background: #e8e8e8 !important;
			}
			/*去掉ul的边框*/
			.mui-table-view:after{
				height:0 !important
			}
			.mui-table-view:before{
				height:0 !important
			}
			/*用户头像*/
			.userimg>.mui-table-view-cell{
				padding:0.32rem 0;
				margin:0 0.32rem;
			}
			.userimg .mui-table-view-cell a{
				padding:0;
				margin:0;
				
			}
			.mui-table-view .mui-media-object{
				width:1.706666rem !important;
				height:1.706666rem !important;
				border-radius:100%;
				max-width:1.706666rem;	
			}
			.mui-table-view .mui-media-object.mui-pull-left{
				margin-right:0.426666rem;
			}
			.mui-media-body{
				display:-moz-box; /* OLD - Firefox 19- (buggy but mostly works) */
          		display:-ms-flexbox;  /* TWEENER - IE 10 */
          		display:-webkit-box; /* OLD - iOS 6-, Safari 3.1-6 */
          		display:-webkit-flex;  /* NEW - Chrome */
          		display: flex; /* NEW, Spec - Opera 12.1, Firefox 20+ */
				flex-flow:column wrap;
				justify-content:center;
				color:#2b2b2b;
				height:1.706666rem;
				line-height:0.426666rem;
				font-size:16px;
			}
			[data-dpr="2"] .mui-media-body{
				font-size:32px;
			}
			[data-dpr="3"] .mui-media-body{
				font-size:48px;
			}
			.mui-media-body p{
				margin-top:0.213333rem;
				line-height:0.346666rem;
				color:#909090;
				font-size:13px;
			}
			[data-dpr="2"] .mui-media-body p{
				font-size:26px;
			}
			[data-dpr="3"] .mui-media-body p{
				font-size:39px;
			}
			/*功能栏*/
			.funcbox>.mui-table-view-cell{
				margin:0 0.32rem;
				padding:0;
				height:1.173333rem;
				line-height:1.173333rem;
			}
			.funcbox>.mui-table-view-cell a{
				margin:0;
				padding:0;
				color:#2b2b2b;
				font-size:16px;
			}
			[data-dpr="2"] .funcbox>.mui-table-view-cell a{
				font-size:32px;
			}
			[data-dpr="3"] .funcbox>.mui-table-view-cell a{
				font-size:48px;
			}
			.mui-table-view .mui-table-view-cell:after{
				background:#e1e1e1;
				left:0;
			}
			/*功能图标*/
			span.iconfont.mui-pull-left{
				color:#0f2646;
				font-size:22px;
			}
			[data-dpr="2"] span.iconfont.mui-pull-left{
				font-size:44px;
			}
			[data-dpr="3"] span.iconfont.mui-pull-left{
				font-size:66px;
			}
			.mui-table-view .mui-table-view-cell span{
				margin-right:0.426666rem;
			}
			/*未登录*/
			.login-sign{
				color:#2b2b2b !important;
				font-size:16px !important;
				line-height:0.426666rem !important;
			}
			[data-dpr="2"] .login-sign{
				font-size:32px !important;
			}
			[data-dpr="3"] .login-sign{
				font-size:48px !important;
			}
			
			
		</style>
	</head>

	<body>
		<div class="mui-content">
		    <ul class="userimg mui-table-view mui-table-view-chevron">
				<li class="mui-table-view-cell mui-media">
					<a class="" href="#">
						<img class="mui-media-object mui-pull-left head-img" id="head-img" src="../Img/mine/login/portrait2.png">
						<div id="after-login" style="display:none">
							<div class="mui-media-body mui-ellipsis" >
								清云 
							   <p class='mui-ellipsis'>溯源号：<b>mouseshit</b></p>
							</div>
						</div>
						<div id="before-login" style="display:block">
							<div class="login-sign mui-media-body">
							    注册/登录
							</div>
						</div>
					</a>
				</li>
			</ul>
		    <ul class="funcbox mui-table-view mui-table-view-chevron" style="margin-top:0.32rem">
				<li class="mui-table-view-cell">
					<a href="#notifications" ><span class="iconfont mui-pull-left">&#xe67f;</span>分享应用</a>
				</li>
				<li class="mui-table-view-cell">
					<a href="#privacy"><span class="iconfont mui-pull-left">&#xe711;</span>意见反馈</a>
				</li>
				<li class="mui-table-view-cell">
					<a href="#general"><span class="iconfont mui-pull-left">&#xe67d;</span>投诉</a>
				</li>
			</ul>
		    <ul class="funcbox mui-table-view mui-table-view-chevron" style="margin-top:0.32rem">
				<li class="mui-table-view-cell">
					<a href="#account1"><span class="iconfont mui-pull-left">&#xe697;</span>关于溯源</a>
				</li>
			</ul>
			<ul class="funcbox mui-table-view mui-table-view-chevron" style="margin-top:0.32rem">
				<li class="mui-table-view-cell">
					<a href="#account2"><span class="iconfont mui-pull-left">&#x3434;</span>设置</a>
				</li>
			</ul>  
		</div>
		<script src="../js/mui.min.js"></script>
		<script type="text/javascript">
			mui.init();
			mui.plusReady(function(){
				//打开登陆注册页面
				document.getElementById("before-login").addEventListener("tap",function(){
					mui.openWindow({
							id:'login/login.html',
							url:'login/login.html'
						})
				})
				
				mui(".mui-table-view").on("tap","a",function(){
					if (this.getAttribute('href') =="#notifications") {
						mui.openWindow({
							id:'share',
							url:'./mine/share.html'
						})
					}else if(this.getAttribute('href') =="#privacy"){
						mui.openWindow({
							id:'feedback.html',
							url:'./mine/feedback.html'
						})
					}else if(this.getAttribute('href') =="#general"){
						mui.openWindow({
							id:'complaint.html',
							url:'./mine/complaint.html'
						})
					}else if(this.getAttribute('href') =="#account1"){
						mui.openWindow({
							id:'about.html',
							url:'./mine/about.html'
						})
					}else if(this.getAttribute('href') =="#account2"){
						mui.openWindow({
							id:'setting.html',
							url:'./setting-index/setting.html'
						})
					}	
				})
				//分享应用	
			})
		</script>
	</body>

</html>